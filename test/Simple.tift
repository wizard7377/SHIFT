// Metamath axioms
* ;

// MP
[A B _ _]{* * (=> A B) A B} ;

// Introduction of a precedent
//[A B]{* * (=> A (=> B A))} ;

// Transposition
[A B C]{* * * (=> (=> A (=> B C)) (=> (=> A B) (=> A C)))} ;


;;; 


// Introduction of the primitive Λ
[x A _]{* * (Λ x A) [x]{* A}} ;

// Splitting of the Λ
(Λ x Λ A Λ B (=> (Λ x (=> A B) (=> (Λ x A) (Λ x B))))) ;

(Λ A Λ B (=> (<=> A B) (=> A B))) ;

(Λ A Λ B (=> (<=> A B) (=> B A))) ;


;;;


// Simple equality
(Λ x (= x x)) ;

(Λ x Λ y (<=> (= x y) (= y x))) ;

(Λ x Λ y Λ z (=> (= x y) (=> (= y z) (= x z)))) ;


;;;


// Zero nat 
(Nat 0) ;

(Λ n (=> (Nat n) (Nat (S n)))) ;

(Λ n (=> (Nat n) (> (S n) 0))) ;

(Λ n (=> (Nat n) (= (+ n 0) n))) ;

(Λ n Λ m (=> (Nat n) (=> (= m n) (Nat m)))) ;


;;;

(= 1 (S 0)) ;

(= 2 (S 1)) ;

(= 3 (S 2)) ;

