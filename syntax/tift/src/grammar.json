{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "tift",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "theory"
      }
    },
    "theory": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "sentence"
          }
        },
        {
          "type": "STRING",
          "value": ";;;"
        }
      ]
    },
    "sentence": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "term"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "term": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "lamed"
        },
        {
          "type": "SYMBOL",
          "name": "cons"
        },
        {
          "type": "SYMBOL",
          "name": "atomic"
        }
      ]
    },
    "cons": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "term"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "lamed": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "term"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "term"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "atomic": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "wildcard"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "wildcard": {
      "type": "STRING",
      "value": "_"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[^_\\(\\)\\[\\]\\{\\}\\;]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}