{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tift",
      "camelcase": "Tift",
      "title": "Tift",
      "scope": "source.tift",
      "file-types": [
        "tift"
      ],
      "injection-regex": "^tift$",
      "class-name": "TreeSitterTift"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tift grammar for tree-sitter",
    "authors": [
      {
        "name": "Asher Frost"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-tift"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}